<?xml version="1.0"?>
<block>
  <name>vector_sink_cn</name>
  <key>nTSDR_Local_vector_sink_cn</key>
  <category>nTSDR_Local</category>
  <import>import nTSDR_Local</import>
  <make>nTSDR_Local.vector_sink_cn($vlen, $finite, $nsamp)</make>
	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>
	<param>
		<name>Finite</name>
		<key>finite</key>
		<value>False</value>
		<type>bool</type>
	</param>
	<param>
		<name>Length</name>
		<key>nsamp</key>
		<value>0</value>
		<type>int</type>
	</param>

	<check>$vlen &gt; 0</check>
	<sink>
		<name>in</name>
		<type>complex</type>
		<vlen>$vlen</vlen>
	</sink>
</block>